<%- include('header')%>

          <!-- Content wrapper -->
          <div class="content-wrapper">
            <!-- Content -->

            <div class="container-xxl flex-grow-1 container-p-y">
              
              
              <!-- Basic Bootstrap Table -->
              <div class="card">
                <h5 class="card-header">cart data</h5>
                <div class="table-responsive text-nowrap">
                  <table class="table">
                    <thead>
                      <tr>
                        <th>no</th>
                        <th>user email</th>
                        <th>movie name </th>
                        <th>movie date</th>
                        <th>movie time</th>
                        <th>movie seat</th>
                        <th>movie total</th>
                      
                      </tr>
                    </thead>
                    <tbody class="table-border-bottom-0">
                      <% var no = '1'; for(var c of cartdata) {%>                      
                        <tr>
                          <th scope="row"><%= no ++ %></th>
                          <td><%= c.email %></td>
                          <td><%= c.moviename %></td>                                              
                          <td><%= c.date %></td>                                              
                          <td><%= c.time %></td>                                              
                          <td><%= c.seats %></td>                                              
                          <td><%= c.total %></td>                                              
                          
                        </tr>
                        <% } %>
                    </tbody>
                  </table>                 
                </div>
              </div>
             
              
    <!-- Core JS -->
    <!-- build:js assets/vendor/js/core.js -->
    <%- include('footer')%>
  </body>
</html>
